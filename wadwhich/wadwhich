use strict;

### main()

our ($quiet, $print_filenames, @wads) = parse_arguments();

our $tmpdir = "/tmp/wadwhich.$$";
mkdir $tmpdir || die("Can't create $tmpdir: $!\n");
our $tmpmap = "$tmpdir/tmp.map";

for my $wad (@wads) {
	$wad = find_wad($wad);
	next unless is_wad($wad);
	our $level = find_first_level($wad);

	if(!$level) {
		warn "$wad doesn't contain any ExMx or MAPxx levels, can't determine gametype\n";
		print $quiet ? "unknown\n" : "Unknown game\n";
	}

	init_textures();
	print "$wad: " if $print_filenames;
	analyze_level($wad, $level, $tmpmap);
	unlink($tmpmap);
}

rmdir($tmpdir);
exit 0;

### end main()

sub usage {
	my $msg = shift || 0;
	warn $msg if $msg;
	warn <<EOF;
Usage: $0 [-q] wadfile [wadfile ...]
EOF
	exit ($msg ? 1 : 0);
}

sub parse_arguments {
	our @ARGV;

	my $quiet = 0;
	if(@ARGV && $ARGV[0] =~ /^-/) {
		my $opt = shift @ARGV;
		if($opt eq '-q') {
			$quiet = 1;
		} else {
			usage("Unknown option $opt\n");
		}
	}
	usage() unless @ARGV;

	return ($quiet, $quiet && (@ARGV > 1), @ARGV);
}

sub find_wad {
	my $wad = shift;
	my @path;

	return $wad if -e "$wad";
	return "$wad.wad" if -e "$wad.wad";

	my $env = $ENV{DOOMWADDIR};

	push @path, $env if $env;
	push @path, "/usr/share/games/doom";

	for(@path) {
		return "$_/$wad" if -e "$_/$wad";
		return "$_/$wad.wad" if -e "$_/$wad.wad";
	}

	return $wad;
}

sub is_wad {
	my $wad = shift;

	if(!open TMP, "<", "$wad") {
	  	warn "$wad: $!\n";
		return 0;
	}

	my $count = read(TMP, my $header, 4);
	close TMP;

	if($count != 4 || $header !~ /^[I|P]WAD/) {
		warn "$wad: not a wad file\n";
		return 0;
	}

	return 1;
}

sub find_first_level {
	my $wad = shift;
	my $level = 0;

	if(!open(LS, '-|', 'lswad', "$wad")) {
	  	warn "$wad: can't lswad: $!\n";
		return 0;
	}

	while(<LS>) {
		chomp;
		if(/^E\dM\d$/) {
			$level = $_;
			last;
		} elsif(/^MAP\d\d$/) {
			$level = $_;
			last;
		}
	}
	close LS;

	return $level;
}

sub analyze_level {
	my ($wad, $level, $tmpmap) = @_;

	our($doom_texture_count, $doom2_texture_count, $heretic_texture_count, $strife_texture_count);
	$doom_texture_count = $doom2_texture_count = $heretic_texture_count = $strife_texture_count = 0;

	system("wadldc", $level, $wad, $tmpmap);
	if(! -e $tmpmap) {
		warn "$wad: Failed to decompile map $level\n";
		return 0;
	}

	if(!open MAP, "<$tmpmap") {
		warn "$tmpmap: $!\n";
		return 0;
	}

	my $found_linedefs = 0;
	my $in_linedefs;
	while(<MAP>) {
		chomp;

		if(/^\s*LEVEL_START\s+\d+\s+\d+\s+(\d+)/) {
			if($1 eq '1') { # wadldc's extended Hexen-only format
				print $quiet ? "hexen\n" : "$wad is for Hexen\n";
				close MAP;
				return 1;
			}
		}

		$in_linedefs = 1, $found_linedefs =1 if(/LINEDEFS_START/);
		$in_linedefs = 0 if(/LINEDEFS_END/);

		if($in_linedefs) {
			analyze_textures($_);
		}
	}

	close MAP;

	if(!$found_linedefs) {
		warn "$wad doesn't contain any linedefs";
		return 0;
	}

	print "$wad is for " unless $quiet;

	if($level =~ /MAP/) { # either doom2 or strife
		if($doom2_texture_count == $strife_texture_count) {
			print $quiet ? "unknown" : "uncertain, either Doom II or Strife?";
		} elsif($doom2_texture_count > $strife_texture_count) {
			print $quiet ? "doom2" : "Doom II";
		} else {
			print $quiet ? "strife" : "Strife";
		}
	} else { # either doom or heretic
		if($doom_texture_count == $heretic_texture_count) {
			print $quiet ? "unknown" : "uncertain, either Doom I or Heretic?";
		} elsif($doom_texture_count > $heretic_texture_count) {
			print $quiet ? "doom" : "Doom I";
		} else {
			print $quiet ? "heretic" : "Heretic";
		}
	}

	print "\n";
}

sub analyze_textures {
	my $line = shift;
	return unless $line =~ s/^\s*S//;

	our($doom_texture_count, $doom2_texture_count, $heretic_texture_count, $strife_texture_count);
	our(%doom_textures, %doom2_textures, %heretic_textures, %strife_textures);

	my @fields = split /\s+/, $line;
	shift @fields for 1..3;

	for(@fields) {
		$doom_texture_count++ if $doom_textures{$_};
		$doom2_texture_count++ if $doom2_textures{$_};
		$heretic_texture_count++ if $heretic_textures{$_};
		$strife_texture_count++ if $strife_textures{$_};
	}
}

